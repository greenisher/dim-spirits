[sub_resource type="Resource" id="Resource_pnhyj"]
script = ExtResource("1_8akr2")
id = "8"
text = "You might espy a little elfling maiden on your journey. Don't think of her as innocent. Oh no! She owes me a whole brace of golden coin, thieved right out from under poor Rags!"
auto_continue = true
auto_continue_node_id = "9"
metadata/_custom_type_script = "uid://x5v5p8f2l6te"

[sub_resource type="Resource" id="Resource_uv4xw"]
script = ExtResource("1_8akr2")
id = "9"
text = "And there's another young lady you might meet. A strange one. She makes health potions, up in the ruined church. Tell her old Rags needs another dozen, for his sickly mother."
auto_continue = true
auto_continue_node_id = "10"
metadata/_custom_type_script = "uid://x5v5p8f2l6te"

[sub_resource type="Resource" id="Resource_p24vj"]
script = ExtResource("1_8akr2")
id = "10"
text = "And a third person...I've never seen their face. Just a steel helmet. Good steel too, thinks old Rags. If something were to...happen to them, go ahead and let old Rags know, will you? I'll make sure that armour goes to a good home."
