[sub_resource type="Resource" id="Resource_0n6x4"]
script = ExtResource("1_8akr2")
id = "3"
text = "Ah yes! Tell your daddy to forgive poor Rags. I didn't mean to drop that hammer on his foot! Now, shouldn't you be off?"
