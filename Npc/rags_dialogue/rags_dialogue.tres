[sub_resource type="Resource" id="Resource_e4kmj"]
script = ExtResource("1_8akr2")
id = "4"
text = "Don't tell me you don't know where to go! To the south is nothing but an old church. You want to go to the bridge. Follow the path to the old town, girl."
